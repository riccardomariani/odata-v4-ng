{
  "name": "odata-v4-ng",
  "version": "1.3.4",
  "license": "MIT",
  "description": "Client side OData V4 typescript library for Angular",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/riccardomariani/odata-v4-ng.git"
  },
  "keywords": [
    "client",
    "odata",
    "odata v4",
    "typescript",
    "angular"
  ],
  "author": "Riccardo Mariani",
  "bugs": {
    "url": "https://github.com/riccardomariani/odata-v4-ng/issues"
  },
  "homepage": "https://github.com/riccardomariani/odata-v4-ng#readme",
  "scripts": {
    "ng": "ng",
    "start": "ng build odata-v4-ng && ng serve",
    "build": "npm run buildAll",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "versioningLib": "cd projects/odata-v4-ng && npm version 1.3.4 --allow-same-version && cd ../..",
    "buildLib": "ng lint odata-v4-ng && ng test odata-v4-ng && ng build odata-v4-ng --prod",
    "buildApp": "ng lint odata-v4-ng-app && ng test odata-v4-ng-app && ng build odata-v4-ng-app --prod --deploy-url=odata-v4-ng/ && rm -R -f docs && mv dist/odata-v4-ng-app docs",
    "versioningApp": "git add -A && git commit -m 'docs: update' && standard-version --release-as 1.3.4",
    "buildAll": "npm run versioningLib && npm run buildLib && npm run buildApp && npm run versioningApp",
    "publishApp": "git push --follow-tags origin master",
    "publishLib": "npm run buildLib && npm publish dist/odata-v4-ng",
    "publishAll": "npm run publishApp && npm run publishLib",
    "release": "cp README.md projects/odata-v4-ng/README.md && npm run buildAll && npm run publishAll"
  },
  "private": false,
  "dependencies": {
    "@angular/animations": "~9.1.1",
    "@angular/cdk": "~9.2.1",
    "@angular/common": "~9.1.1",
    "@angular/compiler": "~9.1.1",
    "@angular/core": "~9.1.1",
    "@angular/forms": "~9.1.1",
    "@angular/platform-browser": "~9.1.1",
    "@angular/platform-browser-dynamic": "~9.1.1",
    "@angular/router": "~9.1.1",
    "@fullcalendar/core": "^4.4.0",
    "angular2-uuid": "^1.1.0",
    "chart.js": "^2.9.3",
    "primeicons": "^2.0.0",
    "primeng": "^9.0.0",
    "quill": "^1.3.7",
    "rxjs": "~6.5.4",
    "tslib": "^1.10.0",
    "zone.js": "~0.10.2"
  },
  "devDependencies": {
    "@commitlint/config-conventional": "~8.3.4",
    "@angular-devkit/build-angular": "~0.901.1",
    "@angular-devkit/build-ng-packagr": "~0.901.1",
    "@angular/cli": "~9.1.1",
    "@angular/compiler-cli": "~9.1.1",
    "@angular/language-service": "~9.1.1",
    "@types/node": "~13.11.1",
    "@types/jasmine": "~3.5.10",
    "@types/jasminewd2": "~2.0.3",
    "husky": "^4.2.5",
    "codelyzer": "^5.1.2",
    "jasmine-core": "~3.5.0",
    "jasmine-spec-reporter": "~4.2.1",
    "rxjs-tslint": "~0.1.8",
    "standard-version": "~8.0.1",
    "karma": "~4.4.1",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage-istanbul-reporter": "~2.1.0",
    "karma-jasmine": "~3.0.1",
    "karma-jasmine-html-reporter": "^1.4.2",
    "ng-packagr": "^9.0.0",
    "protractor": "~5.4.3",
    "ts-node": "~8.3.0",
    "tslint": "~6.1.0",
    "typescript": "~3.8.3"
  }
}
