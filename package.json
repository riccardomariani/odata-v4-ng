{
  "name": "@ebenefitsbv/odata-v4-ng",
  "version": "13.0.3",
  "license": "MIT",
  "description": "Client side OData V4 typescript library for Angular, continuance on the version originally built by Riccardo Mariani",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ebenefits/odata-v4-ng/odata-v4-ng.git"
  },
  "keywords": [
    "client",
    "odata",
    "odata v4",
    "typescript",
    "angular"
  ],
  "author": "eBenefits B.V.",
  "bugs": {
    "url": "https://github.com/ebenefits/odata-v4-ng/issues"
  },
  "homepage": "https://github.com/ebenefits/odata-v4-ng#readme",
  "scripts": {
    "ng": "ng",
    "start": "ng build odata-v4-ng && ng serve",
    "build": "npm run buildAll",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "tag-package-version": "npm version 13.0.3 --allow-same-version --git-tag-version && node scripts/align-package-version-number.js",
    "buildLib": "ncp README.md projects/odata-v4-ng/README.md && ng lint odata-v4-ng && ng test odata-v4-ng && ng build odata-v4-ng --configuration=production",
    "buildApp": "ng lint odata-v4-ng-app && ng test odata-v4-ng-app && ng build odata-v4-ng-app --configuration=production --baseHref=/odata-v4-ng/ && rimraf docs && move-cli dist/odata-v4-ng-app docs",
    "buildAll": "npm run buildLib && npm run buildApp",
    "publish": "npm run tag-package-version && npm run buildLib && cd dist/odata-v4-ng && npm publish --access public && cd ../../"
  },
  "private": false,
  "dependencies": {
    "@angular/animations": "^14.0.0",
    "@angular/cdk": "^14.0.0",
    "@angular/common": "^14.0.0",
    "@angular/compiler": "^14.0.0",
    "@angular/core": "^14.0.0",
    "@angular/forms": "^14.0.0",
    "@angular/platform-browser": "^14.0.0",
    "@angular/platform-browser-dynamic": "^14.0.0",
    "@angular/router": "^14.0.0",
    "@fullcalendar/core": "^4.4.2",
    "angular2-uuid": "^1.1.0",
    "chart.js": "^2.9.4",
    "primeicons": "^5.0.0",
    "primeng": "^14.0.0",
    "rxjs": "^7.5.4",
    "tslib": "^2.4.0",
    "zone.js": "^0.11.7"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^14.0.0",
    "@angular-eslint/builder": "^14.0.0",
    "@angular-eslint/eslint-plugin": "^14.0.0",
    "@angular-eslint/eslint-plugin-template": "^14.0.0",
    "@angular-eslint/schematics": "^14.0.0",
    "@angular-eslint/template-parser": "^14.0.0",
    "@angular/cli": "^14.0.0",
    "@angular/compiler-cli": "^14.0.0",
    "@angular/language-service": "^14.0.0",
    "@types/jasmine": "^4.0.0",
    "@types/jasminewd2": "^2.0.10",
    "@types/node": "^16.11.45",
    "@typescript-eslint/eslint-plugin": "^5.30.7",
    "@typescript-eslint/parser": "^5.30.7",
    "eslint": "^8.20.0",
    "jasmine-core": "^4.3.0",
    "jasmine-spec-reporter": "^7.0.0",
    "karma": "^6.4.0",
    "karma-chrome-launcher": "^3.1.1",
    "karma-coverage": "^2.2.0",
    "karma-jasmine": "^4.0.2",
    "karma-jasmine-html-reporter": "^1.7.0",
    "move-cli": "^2.0.0",
    "ncp": "^2.0.0",
    "ng-packagr": "^14.0.0",
    "protractor": "^7.0.0",
    "rimraf": "^3.0.2",
    "ts-node": "^10.5.0",
    "typescript": "^4.6.4"
  }
}
