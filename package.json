{
  "name": "odata-v4-ng",
  "version": "1.0.11",
  "license": "MIT",
  "description": "Client side OData V4 typescript library for Angular 6",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/riccardomariani/odata-v4-ng.git"
  },
  "keywords": [
    "client",
    "odata",
    "odata v4",
    "typescript",
    "angular",
    "angular 6"
  ],
  "author": "Riccardo Mariani",
  "bugs": {
    "url": "https://github.com/riccardomariani/odata-v4-ng/issues"
  },
  "homepage": "https://github.com/riccardomariani/odata-v4-ng#readme",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "commitmsg": "commitlint -E GIT_PARAMS",
    "buildProject": "npm install && ng lint && ng test --watch=false --code-coverage && ng e2e && ng build --prod --deploy-url=odata-v4-ng/ && rm -R -f docs && mv dist/odata-v4-ng docs",
    "packageProject": "ng-packagr -p ng-package.json",
    "publishProject": "git push --follow-tags origin master && npm publish dist",
    "release": "npm run buildProject && standard-version && npm run packageProject && npm run publishProject"
  },
  "private": false,
  "peerDependencies": {
    "@angular/common": "^6.0.0",
    "@angular/core": "^6.0.0",
    "@angular/http": "^6.0.0",
    "@angular/platform-browser": "^6.0.0",
    "angular2-uuid": "^1.1.1",
    "rxjs": "^6.0.0",
    "zone.js": "^0.8.26"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.6.8",
    "@angular/animations": "^6.0.9",
    "@angular/cli": "~6.0.8",
    "@angular/common": "^6.0.9",
    "@angular/compiler": "^6.0.9",
    "@angular/compiler-cli": "^6.0.9",
    "@angular/core": "^6.0.9",
    "@angular/forms": "^6.0.9",
    "@angular/http": "^6.0.9",
    "@angular/language-service": "^6.0.9",
    "@angular/platform-browser": "^6.0.9",
    "@angular/platform-browser-dynamic": "^6.0.9",
    "@angular/router": "^6.0.9",
    "@commitlint/cli": "^7.0.0",
    "@commitlint/config-conventional": "^7.0.1",
    "@types/jasmine": "~2.8.6",
    "@types/jasminewd2": "~2.0.3",
    "@types/node": "~8.9.4",
    "angular2-uuid": "^1.1.1",
    "codelyzer": "~4.2.1",
    "core-js": "^2.5.4",
    "font-awesome": "^4.7.0",
    "husky": "^0.14.3",
    "jasmine-core": "~2.99.1",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "~1.7.1",
    "karma-chrome-launcher": "~2.2.0",
    "karma-coverage-istanbul-reporter": "~2.0.0",
    "karma-jasmine": "~1.1.1",
    "karma-jasmine-html-reporter": "^0.2.2",
    "ng-packagr": "^3.0.3",
    "primeng": "^6.0.1",
    "protractor": "~5.3.0",
    "rxjs": "^6.2.2",
    "rxjs-tslint": "^0.1.5",
    "standard-version": "^4.4.0",
    "ts-node": "~5.0.1",
    "tslint": "~5.9.1",
    "typescript": "~2.7.2",
    "zone.js": "^0.8.26"
  }
}
